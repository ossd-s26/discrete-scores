{
  "manifest_version": 2,
  "name": "Gradescope Discrete Scores",
  "version": "1.0",

  "description": "Adds privacy functionality to Gradescope.",
  "homepage_url": "https://github.com/ossd-s26/discrete-scores",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },

  "permissions": ["activeTab"],

  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "default_title": "GDS",
    "default_popup": ""
  },
}
